# genfs_contexts.te

# debugfs
genfscon debugfs /wcd_cpe/err_status                                    u:object_r:ssr_crash_reason_file:s0
genfscon debugfs /icnss/crash_reason/wlan_pdr_crash_reason              u:object_r:ssr_crash_reason_file:s0

